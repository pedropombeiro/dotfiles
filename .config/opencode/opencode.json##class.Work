{
  "$schema": "https://opencode.ai/config.json",
  "default_agent": "plan",
  "plugin": ["@gitlab/opencode-gitlab-plugin"],
  "enabled_providers": ["gitlab"],
  "model": "gitlab/duo-chat-opus-4-5",
  "small_model": "gitlab/duo-chat-haiku-4-5",
  "keybinds": {
    "scrollbar_toggle": "ctrl+alt+t",
    "messages_page_up": "pageup,ctrl+alt+b,home",
    "messages_page_down": "pagedown,ctrl+alt+f,end",
    "input_buffer_home": "none",
    "input_buffer_end": "none",
    "messages_first": "ctrl+g",
    "messages_last": "ctrl+alt+g"
  },
  "tui": {
    "scroll_speed": 3,
    "scroll_acceleration": {
      "enabled": true
    }
  }
}
