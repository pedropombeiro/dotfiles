#!/usr/bin/env bash

if [[ -d ${GDK_ROOT} ]]; then
  zsh-defer source "${GDK_ROOT}/support/completions/gdk.bash"

  source ~/.pgsql_profile

  alias compile_docs="bundle exec rake gitlab:graphql:compile_docs"
  alias guard="bundle exec guard"

  # unalias aliased being defer-loaded by git-extras
  zsh-defer unalias gswm
  zsh-defer unalias gpsup
fi
