#!/usr/bin/env zsh

if [[ -d ${GDK_ROOT} ]]; then
  autoload -Uz bashcompinit && bashcompinit
  source "${GDK_ROOT}/support/completions/gdk.bash"

  source ~/.pgsql_profile

  alias compile_docs="bundle exec rake gitlab:graphql:compile_docs"
  alias guard="bundle exec guard"

  # unalias aliases defined by git-extras
  unalias gswm 2>/dev/null
  unalias gpsup 2>/dev/null
fi
